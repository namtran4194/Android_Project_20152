<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="example.phanmemquanlythuchi"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name="com.android.tools.fd.runtime.BootstrapApplication"
        android:fullBackupContent="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity android:name="example.phanmemquanlythuchi.Login" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="example.phanmemquanlythuchi.MainActivity"
            android:label="@string/app_name" />
        <activity
            android:name="example.phanmemquanlythuchi.TienChi"
            android:label="Ghi chép chi tiêu"
            android:parentActivityName="example.phanmemquanlythuchi.MainActivity" />
        <activity
            android:name="example.phanmemquanlythuchi.TienThu"
            android:label="Ghi chép thu nhập"
            android:parentActivityName="example.phanmemquanlythuchi.MainActivity" />
        <activity
            android:name="example.phanmemquanlythuchi.DanhSachThuChi"
            android:label="Danh sách thu chi"
            android:parentActivityName="example.phanmemquanlythuchi.MainActivity" />
        <activity
            android:name="example.phanmemquanlythuchi.BaoCaoThuChi"
            android:label="Thống kê - Biểu đồ"
            android:parentActivityName="example.phanmemquanlythuchi.MainActivity" />
        <activity
            android:name="example.phanmemquanlythuchi.NgoaiTe"
            android:label="Quy đổi tiền tệ"
            android:parentActivityName="example.phanmemquanlythuchi.MainActivity" />
        <activity
            android:name="org.achartengine.GraphicalActivity"
            android:parentActivityName="example.phanmemquanlythuchi.BaoCaoThuChi"
            android:theme="@style/addTheme" />
        <activity
            android:name="example.phanmemquanlythuchi.LaiXuat"
            android:label="Lãi xuất ngân hàng"
            android:parentActivityName="example.phanmemquanlythuchi.MainActivity" />
        <!--
            <activity
            android:name=".CircleActivity"
            android:label="MENU" />
        -->
    </application>

</manifest>