<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/RelativeLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true">

        <EditText
            android:id="@+id/editText_nhapthang"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10">

            <requestFocus />
        </EditText>

        <ImageButton
            android:id="@+id/imageButton_chonthang"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/iconngaythang"
            android:src="@drawable/iconngaythang" />
    </LinearLayout>

    <ImageButton
        android:id="@+id/imageButton_baocaothang"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/linearLayout1"
        android:background="@drawable/icon_ok_small"
        android:src="@drawable/icon_ok_small" />

    <ListView
        android:id="@+id/listView_baocaothang"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/imageButton_baocaothang" />

</RelativeLayout>